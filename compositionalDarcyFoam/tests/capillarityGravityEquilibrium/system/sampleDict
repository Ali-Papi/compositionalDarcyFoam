sets                          
(
    acrossFlow
    {
        type            uniform;
        axis            distance;
        start           (0.05 5e-4 0.05);
        end             (0.05 0.9995 0.05);
        nPoints         1000;
    }
);

surfaces                      ( );

surfaceFormat null;

