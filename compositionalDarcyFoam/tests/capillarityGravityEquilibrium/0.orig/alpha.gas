FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions                    [0 0 0 0 0 0 0];
internalField                 uniform  0.499;
boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 0.4995;
    }
    bottom
    {
        type                          zeroGradient;
    }
    walls
    {
        type                          zeroGradient;
    }
}


