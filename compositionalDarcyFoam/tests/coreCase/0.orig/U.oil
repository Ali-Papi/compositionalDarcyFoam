FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U.oil;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type                fluxCorrectedVelocity;
        phi                 phi.oil;
        rho                 rho.oil;
        value               $internalField;
    }
    inlet
    {
        type                fixedValue;
        value               $internalField;
    }    
    wall
    {
        type                fixedValue;
        value               uniform (0 0 0);
    }
}

