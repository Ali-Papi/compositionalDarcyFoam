FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type basicMultiphaseSystem;

phases (oil water rock);

referencePhase water;

oil
{
    type            pureIsothermalPhaseModel;
    rho0            800;
    psi             0;
    mu              0.002;
    residualAlpha   1e-6;
}

water
{
    type            pureIsothermalPhaseModel;
    rho0             1000;
    psi             0;
    mu              0.001; 
    residualAlpha   1e-6;
}

rock
{
    type            purePorousIsothermalPhaseModel;
    rho0            2713;
    psi             0;
    residualAlpha   1e-6;
}

momentum
{
    oil_relativeWith_rock
    {
        type        Darcy;
        KrModel     BrooksAndCorey;
        eta         2;
    }

    water_relativeWith_rock
    {
        type        Darcy;
        KrModel     BrooksAndCorey;
        eta         2;
    }

}
massTransfer { }
capillarity
{
    water_relativeWith_oil
    {
        type        BrooksAndCorey;
        pc0         100e-8;
        zeta        0.5;
        eta         2;
    }
}
