FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type basicMultiphaseSystem;

phases (water oil rock);

referencePhase water;

oil
{
    type            pureIsothermalPhaseModel;
    rho0            800;
    psi             0;
    mu              0.002;
    residualAlpha   1e-10;
}

water
{
    type            pureIsothermalPhaseModel;
    rho0            1000;
    psi             0;
    mu              0.001;
    residualAlpha   1e-10;
}

rock
{
    type            purePorousIsothermalPhaseModel;
    rho0            2713;
    psi             0;
    residualAlpha   1e-10;
}

momentum
{
    oil_relativeWith_rock
    {
        type        Darcy;

        KrModel     BrooksAndCorey;
        eta         2;
    }

    water_relativeWith_rock
    {
        type        Darcy;

        KrModel     BrooksAndCorey;
        eta         2;
    }
}

massTransfer{}
capillarity{}

