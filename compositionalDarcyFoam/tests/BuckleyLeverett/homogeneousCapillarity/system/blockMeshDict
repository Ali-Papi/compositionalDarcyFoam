FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

x  0.035;
y  0.065;
z  0.035;

nx 1;
ny 500;
nz 1;

vertices
(
    (0  0  0)
    ($x 0  0)
    ($x $y 0)
    (0  $y 0)
    (0  0  $z)
    ($x 0  $z)
    ($x $y $z)
    (0  $y $z)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($nx $ny $nz) simpleGrading (1 1 1)
);

edges
();

boundary
(
    outlet
    {
        type patch;
        faces
        (
            (7 3 2 6)
        );
    }
    
    inlet
    {
        type patch;
        faces
        (
            (0 4 5 1)
        );
    }
    
    walls
    {
        type wall;
        faces
        (
            (0 4 7 3)
            (1 2 6 5)
            (0 3 2 1)
            (4 5 6 7)
        );
    }
);

mergePatchPairs
();

