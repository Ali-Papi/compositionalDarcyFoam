FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    object      U.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type                fixedValue;
        value               uniform (0 -0.0005 0);
    }    

    outlet
    {
        type                fluxCorrectedVelocity;
        phi                 phi.water;
        rho                 rho.water;
        value               $internalField;
    }

    walls
    {
        type                fixedValue;
        value               $internalField;
    }
}

