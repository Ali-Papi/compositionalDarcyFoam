FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type basicMultiphaseSystem;

phases (oil gas rock);

oil
{
    type            pureIsothermalPhaseModel;
    rho0            800;
    psi             0;
    mu              0.002;
    residualAlpha   1e-8;
}

gas
{
    type            pureIsothermalPhaseModel;
    rho0            20;
    psi             0;
    mu              0.001;
    residualAlpha   1e-8;
}

rock
{
    type            multiComponentPorousIsothermalPhaseModel;
    species         2(CO3 SiO3);
    rho0.SiO3       1600;
    rho0.CO3        2711;
    psi.SiO3        0;
    psi.CO3         0;   
    residualAlpha   1e-8;
}

momentum
{
    oil_relativeWith_rock
    {
        type        Darcy;
        KrModel     BrooksAndCorey;
        eta         2;
    }

    gas_relativeWith_rock
    {
        type        Darcy;
        KrModel     BrooksAndCorey;
        eta         2;
    }
}

massTransfer{}

capillarity
{

}

